pipeline {
    agent any 
    stages{
        stage("Build"){
            steps{
                git branch: 'main', credentialsId: 'Boxfuse', url: 'https://github.com/Raksham7/spring-framework-petclinic.git'
            }
        }
        stage("UNIT Testing"){
            steps{
                sh 'mvn test' 
            }
        }        
    }
}
